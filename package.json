{
  "name": "my-fullstack-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://allisondt.github.io/allisonsblog/",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm:open\" \"ts-node-dev --respawn --transpile-only backend/src/index.ts\"",
    "open": "open http://localhost:5173",
    "build": "npm install --legacy-peer-deps && npm run backend-build && vite build",
    "backend-build": "tsc --project backend/tsconfig.json",
    "preview": "vite preview",
    "start": "node backend/dist/index.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.16.4",
    "@mui/material": "^5.16.4",
    "@vitejs/plugin-react": "^4.3.1",
    "axios": "^1.7.2",
    "bootstrap": "^5.3.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongoose": "^5.13.22",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.25.1",
    "vite": "^5.3.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^14.14.37",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "concurrently": "^6.0.0",
    "gh-pages": "^6.1.1",
    "open": "^8.0.9",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
